{
  "agent": {
    "model": "openrouter/nvidia/nemotron-3-nano-30b-a3b:free",
    "workspace": "/home/node/clawd",
    "skipBootstrap": false
  },
  "providers": {
    "openrouter": {
      "apiKey": "${OPENROUTER_API_KEY}"
    }
  },
  "gateway": {
    "bind": "0.0.0.0",
    "port": 18789,
    "auth": {
      "mode": "token",
      "token": "${GATEWAY_TOKEN}"
    }
  },
  "channels": {
    "telegram": {
      "enabled": false
    },
    "discord": {
      "enabled": false
    }
  },
  "tools": {
    "web": {
      "search": {
        "enabled": false
      }
    }
  }
}